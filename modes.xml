<modes>
  <mode name="sl-it" install="yes">
    <pipeline>
      <program name="lt-proc">
        <file name="sl-it.automorf.bin"/>
      </program>
      <program name="apertium-tagger -g">
        <file name="sl-it.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="lt-proc -b">
        <file name="sl-it.autobil.bin"/>
      </program>
      <program name="apertium-transfer -b">
        <file name="apertium-sl-it.sl-it.t1x"/>
        <file name="sl-it.t1x.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-sl-it.sl-it.t2x"/>
        <file name="sl-it.t2x.bin"/>
      </program>
      <program name="apertium-postchunk">
        <file name="apertium-sl-it.sl-it.t3x"/>
        <file name="sl-it.t3x.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="sl-it.autogen.bin"/>
      </program>
      <program name="lt-proc -p">
        <file name="sl-it.autopgen.bin"/>
      </program>
    </pipeline>
  </mode>

